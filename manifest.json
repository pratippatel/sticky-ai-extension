{
  "manifest_version": 3,
  "name": "Sticky AI",
  "version": "1.0",
  "description": "Ask AI questions about any webpage.",
  "permissions": ["storage", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
   "web_accessible_resources": [
    {
      "resources": ["style.css", "LogoStickyAI.png"],
      "matches": ["<all_urls>"]
    }
    ]
}
